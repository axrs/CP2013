<%
var weekdays = new Array(7);
weekdays[0] = "Sunday";
weekdays[1] = "Monday";
weekdays[2] = "Tuesday";
weekdays[3] = "Wednesday";
weekdays[4] = "Thursday";
weekdays[5] = "Friday";
weekdays[6] = "Saturday";
%>
<tr>
    <td><%= weekdays[day] %></td>
    <% if (model.servHrs[day]) { %>
    <td><% if(model.servHrs[day].servHrsStart){ %><%= model.servHrs[day].servHrsStart %><% }else { %>00:00<%} %></td>
    <td><% if(model.servHrs[day].servHrsBreakStart){ %><%= model.servHrs[day].servHrsBreakStart %><% }else { %>00:00<%} %></td>
    <td><% if(model.servHrs[day].servHrsBreakEnd){ %><%= model.servHrs[day].servHrsBreakEnd %><% }else { %>00:00<%} %></td>
    <td><% if(model.servHrs[day].servHrsEnd){ %><%= model.servHrs[day].servHrsEnd %><% }else { %>00:00<%} %></td>
    <% } else { %>
    <td>00:00</td>
    <td>00:00</td>
    <td>00:00</td>
    <td>00:00</td>
    <% } %>
</tr>
