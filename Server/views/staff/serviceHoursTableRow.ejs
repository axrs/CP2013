<%
var weekdays = new Array(7);
weekdays[0] = "Sunday";
weekdays[1] = "Monday";
weekdays[2] = "Tuesday";
weekdays[3] = "Wednesday";
weekdays[4] = "Thursday";
weekdays[5] = "Friday";
weekdays[6] = "Saturday";
%>
<tr>
    <td><%= weekdays[day] %></td>
    <td><% if(typeof model.servHrs[day].servHrsStart != 'undefined'){ %><%= model.servHrs[day].servHrsStart %><% } %></td>
    <td><% if(typeof model.servHrs[day].servHrsBreakStart != 'undefined'){ %><%= model.servHrs[day].servHrsBreakStart %><% } %></td>
    <td><% if(typeof model.servHrs[day].servHrsBreakEnd != 'undefined'){ %><%= model.servHrs[day].servHrsBreakEnd %><% } %></td>
    <td><% if(typeof model.servHrs[day].servHrsEnd != 'undefined'){ %><%= model.servHrs[day].servHrsEnd %><% } %></td>
</tr>
