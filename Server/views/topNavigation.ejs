<%
if(!String.prototype.startsWith){
String.prototype.startsWith = function (str) {
return !this.indexOf(str);
}
}


var currentItem = req.url.toLowerCase();
%>

<li class="<% if(currentItem === '/'){ %> active <% } %>"><a href="/">Home</a></li>
<li class="<% if(currentItem.startsWith('/contacts')){ %> active <% } %>"><a href="/contacts">Contacts</a></li>